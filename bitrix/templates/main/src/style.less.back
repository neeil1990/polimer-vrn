						padding-top: 20px;
					}
					&:nth-child(2) {
						padding-top: 30px;
					}
					&.tall {
						padding-top: 90px;
					}
					.line {
						min-height: 80px;
						padding: 40px 0 0 0;
						span {
							top: 0;
							padding: 12px 0;
							font: 500 14px/16px @fira;
							&.g-title {
								top: -50px;
								padding: 0;
							}
							&.span_time {
								left: 0;
								top: 50px;
							}
							&.span_from {
								left: 60px;
								top: 50px;
							}
							&.span_to {
								left: 180px;
								top: 50px;
							}
						}
						&.dateline {
							height: 100px;
						}
						.inp {
							width: 100%;
							&.phone {
								float: none;	
								left: 0;	
								right: auto;
								width: 50%;		
							}
							&.date {
								position: absolute;
								right: 0;
							}
							&.time {
								position: absolute;
								top: 50px;
								&.from {
									right: auto;
									left: 90px;
								}
								&.to {
								}
							}
							input[type=text] {}
						}
						.phone-add {
							position: relative;
							width: 100%;
							height: 40px;
							margin-top: 10px;
						}
						.addphone {
							right: 0;
							&:before {
								display: none;	
							}
							span {
								display: none;	
							}
						}
					}
				}
				.controls {
					margin-top: 40px;
					.control {
						font: 500 14px/48px @fira;
						&.prev {
							padding-left: 0;
							text-indent: -9999px;
							width: 90px;
							&:before {    
								left: 50%;
								margin-left: -12px;
							}
						}
						&.next {
							width: 180px;
							padding-left: 20px;
							&:before {
								right: 15px;
							}
						}
					}
				}
			}
		}
		&.s3 {
			.payment_types {
				width: 100%;
				.type {
					&.t3 {
						margin-left: 0;
						margin-top: 20px;
					}
					&.t4 {
						margin-top: 20px;
					}
				}
			}
			.issue {
				width: 100%;
			}
		}
		&.s4 {
			.confirm {
				width: 100%;
				.block {
					padding-bottom: 30px;
					margin-bottom: 30px;
					.title {
						margin-top: 0;
						margin-bottom: 10px;
					}
					.receiver {
						display: block;
						margin: 0 0 10px;
						height: 40px;
						width: 100%;
						padding: 0 0;
						font: 500 14px/36px @fira;
						&.byother {
							margin-left: 0;
						}
					}
					.line {
						margin-top: 10px;
						padding-left: 120px;
						font: 400 14px/18px @fira;
						.name {
							br {
								&.translate {
									display: block;
								}
							}
						}
					}
					.rogerdat {
						margin-top: 30px;
						height: auto;
						input[type="checkbox"] + span {
							display: block;
						}
						input[type="checkbox"] + span::before {
							top: 0;
							left: 0;
							margin-top: 0;
						}
					}
					.phonetime {
						height: auto;
						margin-top: 30px;
						span {
							&.label {
								display: block;
								margin: 0 0 5px 0;
							}
						}
					}
					.note {
						margin-top: 30px;
					}
					.controls {
						margin-top: 20px;
						height: 95px;
						.btn-back {
							width: 100%;
							height: auto;
							margin: 0;
							border: none;
							text-align: center;
						}
						.btn-confirm {
							width: 100%;
							height: 50px;
							top: auto;
							bottom: 0;
						}
					}
				}
			}
		}
	}
}


