			font-size: 1.714em;
			color: #0358a6;
			cursor: pointer;
			border-bottom: 1px dotted #0358a6;
			-moz-transition: all 0.3s ease;
			-o-transition: all 0.3s ease;
			-webkit-transition: all 0.3s ease;
			transition: all 0.3s ease;
			&:nth-child(2) {
				margin: 0 70px;
			}
			&:hover, &:active {
				color: #00a1f2;
				border-bottom: 1px dotted #00a1f2;
				-moz-transition: all 0.3s ease;
				-o-transition: all 0.3s ease;
				-webkit-transition: all 0.3s ease;
				transition: all 0.3s ease;
			}
			&.active {
				color: #414141;
				border: 0;
				&:hover, &:active {
					color: #414141;
					border: 0;
				}
			}
		}
	}
	.tab_content {
		position: relative;
		min-height: 480px;
		.tab_item {
			 position: absolute;
			 top: 0;
			 left: 0;
			 width: 100%;
			 display: none;
			 opacity: 0;

			 &.ac{
			 	display: block;
  				opacity: 1;
			 }

		}
	}
}

.slider_product {
	border: 1px solid #cdd2d6;
	.slick-arrow {
		position: absolute;
		display: block;
		margin: 0;
		padding: 0;
		width: 50px;
		height: 60px;
		top: 50%;
		margin-top: -30px;
		text-indent: -9999px;
		cursor: pointer;
		border: none;
		outline: none;
		-webkit-border-radius: 0;
		border-radius: 0;
		background-color: rgb(229,239,248);
		-webkit-backface-visibility: visible;
		background-image: url('img/product_arrow.png');
		-webkit-transition: background-color 200ms ease;
		-moz-transition: background-color 200ms ease;
		-ms-transition: background-color 200ms ease;
		-o-transition: background-color 200ms ease;
		transition: background-color 200ms ease;
		&:hover {
			background-color: #00a1f2;
			background-image: url('img/product_arrow.png');
		}
		&.slick-prev {
			left: -51px;
			background-position: 0 0;
			&:hover {
				background-position: 0 60px;
			}
		}
		&.slick-next {
			right: -51px;
			background-position: -55px 0;
			&:hover {
				background-position: -55px 60px;
			}
		}
	}
	.slick-dots {
		position: absolute;
		width: 100%;
		bottom: -50px;
		text-align: center;
		padding: 0;
		margin: 0;
		li {
			display: inline-block;
			list-style-type: none;
			&:first-child {
				button {
					margin: 0;
				}
			}
			button {
				display: block;
				cursor: pointer;
				margin-left: 15px;
				width: 12px;
				height: 12px;    
				background: rgb(138,147,152);
				outline: none;
				border: none;
				-webkit-border-radius: 10px;
				border-radius: 10px;
				text-indent: -9999px;
				&:hover, &:active {
					background: rgb(4,100,187);
				}
			}
			&.slick-active {
				button {
					background: rgb(4,100,187);
				}
			}
		}
	}
	.product {
		padding: 30px 20px 30px 30px;
		border-left: 1px solid #cdd2d6;
		position: relative;
		margin-left: -1px;
		.name {
			color: #0464bb;
			text-decoration: none;
			margin: 40px 0 15px;
			display: block;
			&:hover, &:active {
				color: #17C3E6;
			}
		}
		.price {
			font-size: 1.142em;
			margin-bottom: 15px;
			span {
				font-weight: 600;
			}
		}
		.cart {
			color: #ffffff;
			background-color: #0464bb;
			background: #0464bb url(img/product_cart.png) no-repeat left center;
			background-position: 15px center;
			text-decoration: none;
			padding: 12px 25px 12px 50px;
			border-radius: 5px;
			display: inline-block;
			-moz-transition: all 0.3s ease;
			-o-transition: all 0.3s ease;
			-webkit-transition: all 0.3s ease;
			transition: all 0.3s ease;
			&:hover, &:active {
				color: #fff;
				background-color: #17C3E6;
			}
		}
		.stock {
			color: #00A600;
			position: relative;
			display: none;
			margin-top: 20px;
			&:after {
				content: '';
				display: block;
				position: absolute;
				left: 125px;
				top: 0;
				width: 18px;
				height: 18px;
				border-radius: 50%;
				background-color: #00A600;
				background-image: url(img/product_stock.png);
				background-repeat: no-repeat;
				background-position: center;
			}
		}
	}
}

