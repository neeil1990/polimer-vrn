.prod_card {
	padding: 30px 0 50px;
	h2{
		margin: 40px 0 20px 0;
	}
}

.pc__prod-info {
	float: left;
	width: 830px;
}

.pc__code {
	display: inline-block;
	font: 400 14px/20px @fira;
	text-align: left;
	color: #414141;
}

.pc__prod-gallery {
	float: left;
	display: table;
	width: 510px;
	padding: 40px 0 30px;
	.pg-thumbnails {
		display: table-cell;
		vertical-align: middle;
		width: 66px;
		.item {
			padding: 2px;
			margin-top: 15px;
			border: 1px solid transparent;
			-webkit-transition: all .2s ease;
			-moz-transition: all .2s ease;
			-ms-transition: all .2s ease;
			-o-transition: all .2s ease;
			transition: all .2s ease;
			a {
				display: table;
				width: 60px;
				height: 60px;
				border: 1px solid #CDD2D6;
