.pc__buy-block {
	float: right;
	position: relative;
	width: 320px;
	padding: 40px 0 30px;
	.bb_compare {
		position: absolute;
		left: 0;
		top: -20px;
		font: 400 14px/20px @fira;
		text-align: left;
		color: #666;
		input[type="checkbox"] {
			display:none;            
		}
		input[type="checkbox"] + label {
			position: relative;
			padding-left: 25px;
			font: 400 14px/18px @fira;
			color: #666666;
			cursor: pointer;
		}
		input[type="checkbox"] + label::before {
			position: absolute;
			display: block;
			content: '';
			top: 50%;
			margin-top: -10px;
			height: 18px;
			width: 18px;
			background: url('img/card/compare_check.png') no-repeat;
			-webkit-background-size: 100%;
			-o-background-size: 100%;
			background-size: 100%;
			-ms-background-position-y: 0;
			background-position-y: 0;
		}
		input[type="checkbox"]:checked + label::before {
			-ms-background-position-y: -19px;
			background-position-y: -19px;
		}
		input[type="checkbox"]:disabled + label::before {
			-ms-background-position-y: -38px;
			background-position-y: -38px;
		}
		input[type="checkbox"]:checked:disabled + label::before {
			-ms-background-position-y: -57px;
			background-position-y: -57px;
		}
	}
	.bb_col {
		float: left;
		width: 170px;
		.price {
			padding: 15px 0 20px;
			.price-old {
				font: 400 18px/20px @fira;
				text-decoration: line-through;
				color: #666;
				span {
					font: 700 18px/20px @fira;
				}
			}
			.price-new {
				padding-top: 13px;
				font: 400 20px/22px @fira;
				color: #333;
				span {
					font: 700 30px/32px @fira;
				}
			}
		}
		.cheaper {
			display: block;
			padding: 35px 0 7px;
			font: 400 16px/18px @fira;
			text-decoration: none;
			text-align: left;
			color: #0464BB;
		}
		.sale {
			height: 100px;
			background: url('img/card/sale_backround.png') no-repeat;
			-webkit-background-size: 150px 94px;
			-o-background-size: 150px 94px;
			background-size: 150px 94px;
			-ms-background-position-x: 100%;
			background-position-x: 100%;
			span {
				padding: 15px 0 0 0;
				display: block;
				font: 700 18px/20px @fira;
				text-align: center;
				color: #FF2A2A;
				&:nth-child(2) {
					padding-top: 10px;
					font: 400 12px/16px @fira;
					text-transform: uppercase;
					color: #333;
				}
			}
		}
		.quantity {
			position: relative;
			margin-top: 30px;
			height: 30px;
			a {
				position: absolute;
				display: block;
				width: 30px;
				height: 100%;
				top: 0;
				background: transparent;
				border: 2px solid #0060bf;
				-webkit-border-radius: 15px;
				border-radius: 15px;
				font-size: 22px;
				color: #0060bf;

				&:hover{ border-color: #00A1F2; color: #00A1F2; }


				&.na{ border-color: #adadad; color: #adadad; 
					&:hover{ border-color: #adadad; color: #adadad; }
				}

				&:before{
					content: '';
					position: absolute;
					display: block;
					width: 26px;
					height: 26px;
					left: 0px;
					top: 1px;
					text-align: center;
					line-height: 26px;
				}
				&.minus{

					&:before {	
						content: '-';
					}
				}
				&.plus{
					right: 0;
					&:before {
						content: '+';
					}
				}
			}
			input[type=text] {
				position: absolute;
				margin: 0;
				padding: 0;			
				width: 40px;
				height: 100%;
				left: 50%;
				padding: 5px;
				margin-left: -20px;
				outline: none;
				border: 1px solid #CDD2D6;
				-webkit-border-radius: 5px;
				border-radius: 5px;
				font: 400 14px/20px @fira;
				text-align: center;
				color: #000;
				&:hover, &:focus{ border-color: #0060bf; }
			}
		}
		&.right {
			width: 150px;
		}
	}
	.add2cart {
		float: left;
		display: block;
		position: relative;
		margin-top: 25px;
		padding-left: 70px;
		width: 100%;
		height: 80px;
		background: #0060bf;
		-webkit-border-radius: 5px;
		border-radius: 5px;
		font: 700 18px/78px @fira;
		text-decoration: none;
		text-align: center;
		color: #FFF;
		&:before {
			position: absolute;
			display: block;
			content: '';
			width: 38px;
			height: 32px;
			left: 35px;
			top: 50%;
			margin-top: -16px;
			background: url('img/card/add2cart.png') no-repeat;
			-webkit-background-size: 100%;
			-o-background-size: 100%;
			background-size: 100%;
		}
		&:hover {
			background: #00A1F2;
		}
	}
	.bb_btn {
		float: left;
		display: table;
		width: 150px;
		height: 60px;
		margin-top: 20px;
		text-align: center;
		text-decoration: none;
		border: 1px solid @blue_l;
