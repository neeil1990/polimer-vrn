.popup{ position: fixed; background: #fff; padding: 50px; top: 120px; left:  50%; border-radius: 4px; box-shadow: 0 5px 15px rgba(0,0,0,.55); z-index: 500;
	
	.close{ background: url(img/popup-close-ico.jpg) left top no-repeat transparent; font-size: 0; line-height: 0; position: absolute; top: 30px; right: 30px; width: 22px; height: 22px; display: block;
		&:hover{ background-position: left bottom; }
	}
	&#mailus{ width: 650px; margin: 0 0 0 -325px; display: none; }
	&#oneclick{ width: 600px; margin: 0 0 0 -300px; display: none; }
	&#specialist{ width: 600px; margin: 0 0 0 -300px; display: none; }

	.title{ font-size: 24px; margin: 0 0 20px 0; font-weight: 600; }
	.subtitle{ margin: 0 0 30px 0; }

	form, fieldset{ margin: 0; padding: 0; border: 0; }

	input[type=text]{ width: 100%; height: 50px; line-height: 48px; border: 1px solid #cdd2d6; border-radius: 4px; padding: 0 10px;
		&.phone{ width: 170px; }

		&:hover, &:focus{ border-color: #0060bf; }
	}

	input[type=submit]{ width: auto; height: 60px; background: #0060bf; color: #fff; padding: 0 40px; border: 0; border-radius: 4px; font-size: 24px; 
		&:hover{ background-color: #00a1f2; }
	}

	textarea{ width: 100%; padding: 10px; border: 1px solid #cdd2d6; border-radius: 4px; resize: none; height: 170px;
		&:hover, &:focus{ border-color: #0060bf; }
	}

	.line{ display: block; margin: 0 0 20px 0;
		.label{ display: block; float: left; font-size: 16px; float: left; width: 150px; padding: 14px 0 0 0; }
		.value{ display: block; margin: 0 0 0 150px; }

		&.wide{
			.label{ float: none; margin: 0 0 5px 0; }
			.value{ margin: 0; }
		}
		&.submit{ text-align: center; }
	}
}

@media screen and (max-width: 1019px) {
	.popup{ width: 100% !important; height: 100% !important; left: 0; top: 0; padding: 70px; margin: 0 !important; }
}

@media screen and (max-width: 660px) {
	.popup{ padding: 60px 20px; overflow-y: auto;
		.line{ margin: 0 0 10px 0;
			.label{ width: auto; float: none; margin: 0 0 5px 0; padding: 0; }
