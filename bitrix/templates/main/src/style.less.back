/* ============= NEWS =============*/

.ns__block {
	.item {
		display: inline-block;
		vertical-align: top;
		position: relative;
		width: 297px;
		padding-right: 30px;
		padding-bottom: 35px;
		margin-bottom: 35px;
		.date {
			font: italic 12px/14px @fira;
			text-align: left;
			color: #737373;
		}
		.title {
			display: inline-block;
			margin-top: 5px;
			border-bottom: none;
			font: 400 16px/20px @fira;
			text-decoration: none;
			text-align: left;
			color: @blue_l;
			&:hover, &:active {
				color: #1B94E9;
				border-color: #1B94E9;
			}
		}
		.txt {
			margin-top: 10px;
			font: 400 12px/14px @fira;
			text-align: left;
			color: #414141;
		}
		&:after {
			position: absolute;
			display: block;
			content: '';
			width: 40px;
			height: 2px;
			bottom: 0;
			left: 0;
			background: #CED5D9;
		}
		&:nth-last-child(-n+4) {
			&:after {
				display: none;
			}
		}
	}
}

.ns__paginator {
	.name {
		float: left;
		margin-right: 30px;
		font: 500 14px/30px @fira;
		text-align: left;
		color: #333;
	}
	.arrow {
		float: left;
		position: relative;
		display: block;
		width: 30px;
		height: 30px;
		cursor: default;
		border: 1px solid #CED5D9;
		-webkit-border-radius: 15px;
		border-radius: 15px;
		&:hover, &:active {
			border-color: #CED5D9;
			span {
				background: #CED5D9;
			}
		}
		span {
			position: absolute;
			display: block;
			width: 5px;
			height: 1px;
			background: #CED5D9;
			top: 50%;
			margin-top: 1px;
			left: 50%;
			margin-left: -2.5px;
			-webkit-transform: rotate(45deg);
			-moz-transform: rotate(45deg);
			-ms-transform: rotate(45deg);
			-o-transform: rotate(45deg);
			transform: rotate(45deg);
			&:nth-child(2) {
				margin-top: -2px;
				-webkit-transform: rotate(-45deg);
				-moz-transform: rotate(-45deg);
				-ms-transform: rotate(-45deg);
				-o-transform: rotate(-45deg);
				transform: rotate(-45deg);
			}
		}
		&.left {
			margin-right: 20px;
		}
		&.right {
			margin-left: 20px;
			span {
				margin-top: -2px;
				margin-left: -1.5px;
				&:nth-child(2) {
					margin-top: 1px;
				}
			}
		}
		&.aractive {
			cursor: pointer;
			border: 1px solid #0060BF;
			span {
				background: #0060BF;
			}
			&:hover, &:active {
				background: #1B94E9;
				border-color: #1B94E9;
				span {
					background: #fff;
				}
			}
		}
	}
	.pages {	
		float: left;
		padding: 3px 0;
		.page {
			float: left;
			display: block;
			margin: 0 6px;
			width: 24px;
			height: 24px;
			border: none;
			border: 1px solid transparent;
			-webkit-border-radius: 12px;
			border-radius: 12px;
			background: transparent;
			font: 400 14px/24px @fira;
			text-align: center;
			color: #0060BF;
			-webkit-transition: all .2s ease;
			-moz-transition: all .2s ease;
			-ms-transition: all .2s ease;
			-o-transition: all .2s ease;
			transition: all .2s ease;
			&.active {
				background: #0060BF;
				color: #fff;
				&:hover, &:active {
					border-color: #1B94E9;
					background: #1B94E9;
				}
			}
			&:hover, &:active {
				border: 1px solid #1B94E9;
				
			}
			&:first-child {
				margin-left: 0;
			}
			&:last-child {
				margin-right: 0;
			}
		}	
	}
	.dots {
		float: left;
		margin: 0 6px;
	}
}

