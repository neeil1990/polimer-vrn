/* ============= COMMON_STYLE =============*/

@blue_l: #0464bb;
@green_l: #157f17;
@fira: 'Fira Sans', sans-serif;

h1 {
	margin: 0 0 30px 0;
	font: 500 24px/28px @fira;
	text-align: left;
	color: #333;
}

body, html {
	background-color: #dfe5e9;
}

* {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	outline: none;
}

body {
	font-family: 'Fira Sans', sans-serif;
	font-size: 14px;
	color: #333333;
	line-height: 1.42;
}

article, aside, details, figcaption, figurefooter, header, hgroup, menu, nav, section {
	display: block;
	position: relative;
}

img {
	max-width: 100%;
	height: auto;
}



.cl:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

.container {
	background-color: #fff;
	width: 1300px;
	margin: 0 auto;
	-webkit-box-shadow: 0px 0px 69px -5px rgba(0, 0, 0, 0.67);
	-moz-box-shadow: 0px 0px 69px -5px rgba(0, 0, 0, 0.67);
	box-shadow: 0px 0px 69px -5px rgba(0, 0, 0, 0.67);
	overflow: hidden;
}

.wr {
	width: 1200px;
	margin: 0 auto;
}

.hidden1300 {
	display: none;
}

.over {
	overflow: hidden;
	.hmobile {
		border-bottom: 1px solid #cdd2d6;
	}
}

.breadcrumbs {
	padding: 15px 0;
	background: #F5F6F8;
	a {
		position: relative;
		display: block;
		float: left;
		margin: 0 20px 0 0;
		border-bottom: 1px solid #7CADD9;
		font: 400 13px/14px @fira;
		text-decoration: none;
		color: @blue_l;
		&:first-child {
			margin-left: 0;
		}
		&:after {
			position: absolute;
			display: block;
			content: '';
			width: 6px;
			height: 9px;
			right: -14px;
			top: 50%;
			margin-top: -5.5px;
			background: url('img/breadcrumbs_arrow.png') no-repeat;
			-webkit-background-size: 100%;
			-o-background-size: 100%;
			background-size: 100%;
		}
	}
	span {
		display: block;
		float: left;
		font: 400 13px/14px @fira;
		color: #414141;
	}
}

.page_content {
	position: relative;
	padding: 30px 0 50px;
	a {
		border-bottom: 1px solid @blue_l;
		text-decoration: none;
		color: @blue_l;
		&:hover, &:active {
			color: #1B94E9;
			border-color: #1B94E9;
		}
	}
	p {
		margin: 20px 0;
		font: 400 14px/20px @fira;
		text-align: left;
		color: #333;
		&.intro {
			margin: 25px 0;
			font: 400 18px/24px @fira;
		}
	}
	ul {
		margin: 30px 0;
		li {
			list-style-type: none;
			position: relative;
			margin-top: 20px;
			font: 400 14px/20px @fira;
			text-align: left;
			color: #333;
			&:first-child {
				margin-top: 0;
			}
			&:before {
				position: absolute;
				display: block;
				content: '';
				width: 18px;
				height: 18px;
				left: -40px;
				top: 0;
				background: url('img/ul-li-mark.png') no-repeat;
				-webkit-background-size: 100%;
				-o-background-size: 100%;
				background-size: 100%;
			}
		}
		&.monocolor {
			li {
				&:before {
					background: url('img/ul-li-mark--monocolor.png') no-repeat;
					-webkit-background-size: 100%;
					-o-background-size: 100%;
					background-size: 100%;
				}
			}
		}
	}
	.row {
		position: relative;
		.col2 {
			float: left;
			width: 50%;
			padding-right: 60px;
			&:nth-child(2) {
				padding-right: 0;
			}
			&>p:first-child {
				margin-top: 0;
				.intro {
					margin-top: 0;
				}
			}
			&.pr40 {
				padding-right: 40px;
			}
			&.pr50 {
				padding-right: 50px;
			}
			&.pl60 {
				padding-left: 60px;
			}
			&.pb180 {
				padding-bottom: 180px;
			}
		}
	}
}

@media screen and (max-width: 1319px) {
	.container {
		width: auto;
	}
	.wr {
		width: 1000px;
		padding-left: 30px;
		padding-right: 30px;
	}
	.hidden1300 {
		display: block;
	}
	.hidden1000 {
		display: none;
	}
	.page_content {
		p {
			margin: 10px 0;
		}	
	}
}

@media screen and (max-width: 1019px) {
	.wr {
		width: 640px;
		padding-left: 0;
		padding-right: 0;
	}
	.hidden1000 {
		display: block;
	}
	.hidden640 {
		display: none;
	}
	.breadcrumbs {
		padding: 10px 0;
		a {
			margin: 5px 20px 5px 0;
		}
		span {
			margin: 5px 0 5px 0;
		}
	}
}

@media screen and (max-width: 659px) {
	h1 {
		margin: 0 0 15px 0;
		font: 600 18px/24px @fira;
	}
	.wr {
		width: 320px;
		padding-left: 20px;
		padding-right: 20px;
	}
	.hidden1000 {
		display: block;
	}
	.hidden320 {
		display: none;
	}
	.breadcrumbs {
		a {
			display: none;
		}
		span {
			display: none;
		}
	}
}


/* ============= HEADER =============*/

header {
	.cart__number {
		position: absolute;
		width: 17px;
		height: 17px;
		text-align: center;
		padding-top: 2px;
		display: inline-block;
		font-size: 0.714em;
		font-weight: 500;
		border-radius: 15px;
		background-color: #ffffff;
		color: #0464bb;
		top: -5px;
		left: 20px;
	}
	.search__input {
		width: 90%;
		border: none;
		height: 100%;
		float: left;
	}
	.search__btn {
		border: none;
		outline: none;
		height: 100%;
		width: 25px;
		background-image: url(img/hbottom_icons.png);
		background-color: transparent;
		cursor: pointer;
		background-position: -168px -13px;
	}
	.header__timework {
		position: relative;
		float: left;
		margin: 0;
		font-weight: 500;
		color: #4c4c4c;
		font-size: 0.857em;
		width: 125px;
		&:before {
			content: '';
			position: absolute;
			width: 32px;
			height: 32px;
			background-position: -93px -172px;
			background-image: url(img/h_icons.png);
			left: -50px;
			top: 8px;
		}
		p {
			margin: 0;
		}
		.line {
			background: url(img/time_dots3.png) repeat left 11px;
			.days {
				float: left;
				background-color: #fff;
				padding-right: 2px;
			}
			.hours {
				float: right;
				background-color: #fff;
				padding-left: 2px;
				.weekend {
					color: #da0000;
				}
			}
		}
	}
	.header__adress {
		position: relative;
		float: right;
		line-height: 40px;
		padding-left: 35px;
		font-size: 0.928em;
		cursor: pointer;
		text-decoration: none;
		color: #4c4c4c;
		&:before {
			width: 16px;
			height: 20px;
			background-position: -20px -20px;
			position: absolute;
			content: '';
			background-image: url(img/h_icons.png);
			left: 0;
			top: 10px;
		}
		&:hover {
			color: #0060bf;
			&:before {
				background-image: url(img/h_icons.png);
				background-position: -76px -20px;
			}
		}
	}
	.header__letter {
		position: relative;
		float: right;
		margin: 0 13px 0 35px;
		padding-left: 40px;
		line-height: 40px;
		font-size: 0.928em;
		cursor: pointer;
		text-decoration: none;
		color: #4c4c4c;
		&:before {
			width: 18px;
			height: 14px;
			background-position: -93px -96px;
			position: absolute;
			content: '';
			background-image: url(img/h_icons.png);
			left: 0;
			top: 13px;
		}
		&:hover {
			color: #0060bf;
			&:before {
				background-image: url(img/h_icons.png);
				background-position: -151px -96px;
			}
		}
	}

	.hmobile {
		display: none;
		height: 100px;
		padding-top: 30px;
		z-index: 201;
		position: relative;
		background-color: #fff;
		.hmobile__logo {
			float: left;
			margin-right: 75px;
		}
		.hmobile__phone {
			color: #333333;
			float: left;
			font-size: 1.5em;
			font-weight: 500;
			padding-top: 5px;
		}
		.hmobile__search {
			width: 25px;
			height: 25px;
			float: left;
			background: url(img/hmobile_search.png);
			margin-left: 60px;
			margin-top: 5px;
		}
		.menu__trigger {
			float: right;
			width: 30px;
			border: 0;
			margin: 5px 0 0 0;
			span {
				display: block;
				height: 2px;
				background: #0464bb;
				margin-bottom: 6px;
				transition: all .2s;
			}
			&.close {
				.i1 {
					transform: translateY(10px) rotate(-135deg);
				}
				.i2 {
					display: none;
				}
				.i3 {
					transform: translateY(3px) rotate(135deg);
				}
			}
		}
		.hmobile__cart {
			position: relative;
			width: 34px;
			height: 28px;
			display: block;
			float: right;
			margin: 5px 40px 0 0;
			background-image: url(img/hmobile_cart.png);
			.cart__number {
				background-color: #0464bb;
				color: #fff;
			}
		}
	}
	.mm__wrap {
		display: none;
		opacity: 0;
		height: 0;
		background: #fff;
		position: fixed;
		left: 0;
		top: 0;
		width: 100%;
		z-index: 200;
		overflow: auto;
		text-align: center;
		padding-top: 120px;
		&.show {
			opacity: 1;
			display: block;
			height: 100%;
		}
		.mm__search {
			-webkit-appearance: none;
			z-index: 1;
			height: 60px;
			width: 65.625%;
			background-color: white;
			box-shadow: none;
			border: 1px solid #E5E5E5;
			display: block;
			position: relative;
			text-align: left;
			padding-left: 20px;
			overflow: hidden;
			float: left;
			font-style: italic;
			.search__input {
				width: 90%;
			}
			.search__btn {
				background-position: -168px -3px;
			}
		}
		.mm__account {
			color: #0464bb;
			font-size: 1.142em;
			text-decoration: none;
			width: 34%;
			font-weight: 500;
			line-height: 60px;
			text-align: center;
			display: block;
			float: right;
		}
		.mm__category {
			margin: 20px 0 0 0;
			padding: 0;
			.maincategory {
				display: table;
				padding: 85px 0 0 0;
				list-style: none;
				width: 31.25%;
				height: 100px;
				float: left;
				margin: 0 0 20px 0;
				background-color: #f4f6f8;
				cursor: pointer;
				position: relative;
				border-top: 1px solid #ced5d9;
				border-left: 1px solid #ced5d9;
				border-right: 1px solid #ced5d9;
				border-bottom: 1px solid #ced5d9;
				&:nth-child(2) {
					margin: 0 20px 20px 20px;
				}
				a {
					text-decoration: none;
					.img {
						position: absolute;
						width: 90px;
						height: 65px;
						top: 15px;
						left: 50%;
						margin-left: -45px;
						margin-top: 0;
					}
					.name {
						color: #0464bb;
						font-weight: 600;
						font-size: 1.1em;
						height: 60px;
						vertical-align: middle;
						display: table-cell;
					}
				}
				&:hover {
					a {
						.name {
							color: #00A1F2;
						}
					}
				}
				&.maincategory--1 {
					a {
						.img {
							background-image: url(img/category_1.png);
						}
					}
					&:hover {
						a {
							.img {
								background-image: url(img/category_1_h.png);
							}
						}
					}
				}
				&.maincategory--2 {
					a {
						.img {
							background-image: url(img/category_2.png);
						}
					}
					&:hover {
						a {
							.img {
								background-image: url(img/category_2_h.png);
							}
						}
					}
				}
				&.maincategory--3 {
					a {
						.img {
							background-image: url(img/category_3.png);
						}
					}
					&:hover {
						a {
							.img {
								background-image: url(img/category_3_h.png);
							}
						}
					}
				}
			}
		}
		.mm__action {
			font-weight: 500;
			font-size: 1.142em;
			width: 48.4375%;
			height: 60px;
			padding-left: 20px;
			border-radius: 5px;
			line-height: 60px;
			float: left;
			text-decoration: none;
			background-color: #0359a6;
			text-align: center;
			color: #ffd400;
			position: relative;
			margin-bottom: 40px;
			&:before {
				display: block;
				content: '';
				position: absolute;
				width: 22px;
				height: 20px;
				background-position: -168px -85px;
				background-image: url(img/hbottom_icons.png);
				top: 20px;
				left: 110px;
			}
		}
		.mm__calculation {
			font-weight: 500;
			width: 48.4375%;
			height: 60px;
			padding-left: 30px;
			border-radius: 5px;
			padding-top: 18px;
			float: right;
			text-decoration: none;
			border: 2px solid #0359a6;
			color: #0359a6;
			text-align: center;
			position: relative;
			margin-bottom: 40px;
			&:before {
				content: '';
				position: absolute;
				width: 32px;
				height: 36px;
				background-position: -132px -20px;
				background-image: url(img/h_icons.png);
				left: 45px;
				top: 10px;
			}
		}
		.mm__phone {
			display: none;
			font-size: 2em;
			color: #333333;
			font-weight: 500;
			text-align: center;
		}
		.mm__timework {
			margin-left: 65px;
		}
		.header__letter {
			margin-left: 75px;
			margin-top: 5px;
			margin-right: 30px;
		}
		.header__adress {
			margin-top: 5px;
		}
		.mm__menu {
			margin: 45px 0 0 0;
			padding: 10px 0;
			border-top: 1px solid #cdd2d6;
			display: inline-block;
			width: 100%;
			text-align: justify;
			&:after {
				content: '';
				width: 100%;
				height: 0;
				display: inline-block;
				text-align: justify;
			}
			li {
				margin: 0;
				padding: 0;
				list-style: none;
				display: inline-block;
				a {
					text-decoration: none;
					color: #4c4c4c;
					border: 0;
					padding: 10px 0;
					line-height: 40px;
					font-size: 0.928em;
				}
				&.active {
					a {
						color: #0060bf;
						-moz-transition: all 0.3s ease;
						-o-transition: all 0.3s ease;
						-webkit-transition: all 0.3s ease;
						transition: all 0.3s ease;
					}
				}
			}
		}
	}
	.header__top {
		background-color: #f4f6f8;
		border-bottom: 1px solid #ced5d9;
		.menu__top {
			margin: 0;
			padding: 0;
			display: inline-block;
			text-align: justify;
			float: left;
			width: 47.5%;
			height: 40px;
			&:after {
				content: '';
				width: 100%;
				height: 0;
				display: inline-block;
				text-align: justify;
			}
			li {
				margin: 0;
				padding: 0;
				list-style: none;
				display: inline-block;
				a {
					text-decoration: none;
					color: #4c4c4c;
					border: 0;
					-moz-transition: all 0.3s ease;
					-o-transition: all 0.3s ease;
					-webkit-transition: all 0.3s ease;
					transition: all 0.3s ease;
					line-height: 40px;
					font-size: 0.928em;
					&:hover, &:active {
						color: #0060bf;
					}
				}
				&.active a {
					color: #0060bf;
					-moz-transition: all 0.3s ease;
					-o-transition: all 0.3s ease;
					-webkit-transition: all 0.3s ease;
					transition: all 0.3s ease;
				}
			}
		}
	}
	.header__main {
		padding-top: 23px;
		.header__logo {
			display: block;
			margin: 0 8% 23px -2px;
			float: left;
			padding-top: 5px;
		}
		.header__phone {
			font-weight: 500;
			font-size: 2em;
			margin: 0 11.5% 0 0;
			float: left;
			padding-top: 7px;
		}
		.header__calculation {
			float: right;
			cursor: pointer;
			position: relative;
			padding-left: 45px;
			color: #0358a6;
			font-weight: 500;
			font-size: 1.142em;
			margin: 0 30px 0 43px;
			padding-top: 5px;
			text-decoration: none;
			&:before {
				content: '';
				position: absolute;
				width: 32px;
				height: 36px;
				background-position: -132px -20px;
				background-image: url(img/h_icons.png);
				left: 0;
				top: 8px;
			}
			&:hover {
				color: #00a1f2;
				&:before {
					background-image: url(img/h_icons.png);
					background-position: -20px -96px;
					width: 33px;
					top: 6px;
					left: -1px;
					height: 40px;
				}
			}
		}
		.header__price {
			float: right;
			cursor: pointer;
			position: relative;
			padding-left: 40px;
			color: #0358a6;
			font-weight: 500;
			font-size: 1.142em;
			padding-top: 15px;
			text-decoration: none;
			&:before {
				content: '';
				position: absolute;
				width: 32px;
				height: 36px;
				background-position: -204px -20px;
				background-image: url(img/h_icons.png);
				left: 0;
				top: 8px;
			}
			&:hover {
				color: #00a1f2;
				&:before {
					background-image: url(img/h_icons.png);
					background-position: -209px -96px;
				}
			}
		}
	}
	.header__bottom {
		height: 60px;
		background: #056ac8;
		position: relative;
		background: -moz-linear-gradient(top, #056ac8 0%, #056ac8 10%, #034f96 100%);
		background: -webkit-gradient(left top, left bottom, color-stop(0%, #056ac8), color-stop(10%, #056ac8), color-stop(100%, #034f96));
		background: -webkit-linear-gradient(top, #056ac8 0%, #056ac8 10%, #034f96 100%);
		background: -o-linear-gradient(top, #056ac8 0%, #056ac8 10%, #034f96 100%);
		background: -ms-linear-gradient(top, #056ac8 0%, #056ac8 10%, #034f96 100%);
		background: linear-gradient(to bottom, #056ac8 0%, #056ac8 10%, #034f96 100%);
		filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#056ac8', endColorstr='#034f96', GradientType=0);
		.header__catalog {
			width: 20%;
			float: left;
			padding: 20px 20px 0 20px;
			margin: 0 48px 0 -20px;
			height: 60px;
			cursor: pointer;
			color: #fff;
			transition: .2s all;

			&:hover{ background: #fff; color: #0358a6; 
				.catalog__trigger {
					span{ background: #0358a6; }
				}
			}

			.catalog__trigger {
				float: left;
				width: 30px;
				border: 0;
				span {
					display: block;
					height: 2px;
					background: #fff;
					margin-bottom: 6px;
					transition: all .2s;
				}
				&.close {
					.i1 {
						transform: translateY(10px) rotate(-135deg);
					}
					.i2 {
						display: none;
					}
					.i3 {
						transform: translateY(0px) rotate(135deg);
					}
				}
			}
			.catalog__name {
				margin: -2px 0 0 20px;
				float: left;
				font-weight: 600;
				font-size: 1.285em;
			}

			.catalog-sections-menu{ display: none; position: absolute; width: 100%; top: 60px; left: 0; min-height: 100px; z-index: 200; width: 100%; text-align: center;
				.wr{ text-align: left; }
				a{ color: #0358a6; }
				ul.first-sections{ display: block; list-style: none; margin: 0 0 0 -20px; padding: 0; width: 240px; background: #fff; box-shadow: 0 10px 15px rgba(0,0,0,.25); position: relative;
					&>li{ font-size: 16px; border-top: 1px solid #cdd2d6;
						&:first-child{ border-top: 0; }
						& > a{ display: block; border: 0; text-decoration: none; font-weight: 600; padding: 30px 0 30px 112px; position: relative;
							&:hover{ color: #333333; }
							img{ position: absolute; left: 20px; top: 27px; width: 70px; height: auto; }
						}



						.subsections{ display: none; top: 0; font-size: 14px; left: 240px; width: 1030px; background: #fff; position: absolute; padding: 40px; box-shadow: 0 10px 15px rgba(0,0,0,.25);
							ul{ list-style: none; padding: 0; margin: 0; 
								li{ position: relative;
									ul{ display: none; padding: 10px 0 0 0; font-size: 13px;
										li{ background: url(img/header-catalog-arr.jpg) left 10px no-repeat transparent; margin: 0 0 10px 0; padding: 0 0 0 20px; }
									}
									a{ text-decoration: none;
										&:hover{ color: #333; }
									}

									.dd{ display: block; width: 14px; height: 14px; text-align: center; border: 2px solid #0358a6; position: absolute; top: 2px; left: 0; font-size: 18px; line-height: 0; padding: 5px 0 0 0; }
								}

								& > li{ margin: 0 0 15px 0; padding: 0 0 0 30px; }
							}

							& > ul{ float: left; width: 33.33%; padding: 0 40px 0 0; }
						}

						&:hover{
							.subsections{ display: block; }
						}
					}
				}
			}

			&:hover{
				.catalog-sections-menu{ display: block; }
			}
		}
		.header__action {
			width: auto;
			float: left;
			position: relative;
			margin: 0 25px 0 0;
			padding: 18px 30px 18px 37px;
			&:before {
				display: block;
				content: '';
				position: absolute;
				width: 22px;
				height: 20px;
				background-position: -168px -85px;
				background-image: url(img/hbottom_icons.png);
				top: 18px;
				left: 0;
			}
			.action__title {
				position: relative;
				color: #ffd400;
				font-weight: 600;
				font-size: 1.285em;
			}
			.action__list {
				display: none;
				width: 200px;
				position: absolute;
				background-color: #fff;
				-webkit-box-shadow: 0px 0px 10px 2px rgba(0, 0, 0, 0.35);
				-moz-box-shadow: 0px 0px 10px 2px rgba(0, 0, 0, 0.35);
				box-shadow: 0px 0px 10px 2px rgba(0, 0, 0, 0.35);
				top: 40px;
				border-radius: 5px;
				left: -40px;
				z-index: 20;
				padding: 15px 10px 10px;
				text-align: center;
				&:before {
					content: '';
					display: block;
					position: absolute;
					width: 0;
					height: 0;
					border-left: 12px solid transparent;
					border-right: 12px solid transparent;
					border-bottom: 12px solid #fff;
					top: -11px;
					left: 50%;
					margin-left: -12px;
				}
				li {
					list-style: none;
					padding: 10px 0;
					border-top: 1px solid #ced5d9;
					&:nth-child(1) {
						border: none;
					}
					a {
						color: #0464bb;
						text-decoration: none;
						font-size: 14px;
						font-weight: 500;
						-moz-transition: all 0.3s ease;
						-o-transition: all 0.3s ease;
						-webkit-transition: all 0.3s ease;
						transition: all 0.3s ease;
						padding: 10px 0;
						&:hover {
							color: #00a1f2;
							-moz-transition: all 0.3s ease;
							-o-transition: all 0.3s ease;
							-webkit-transition: all 0.3s ease;
							transition: all 0.3s ease;
						}
					}
				}
			}
			.action__arrow {
				display: block;
				width: 10px;
				position: absolute;
				height: 6px;
				background-position: -20px -88px;
				background-image: url(img/hbottom_icons.png);
				top: 27px;
				right: 5px;
			}
			&:hover {
				.action__list {
					display: block;
				}
			}
		}
		.header__search {
			width: 31.666%;
			float: left;
			margin: 10px 50px 0 0;
			.search {
				-webkit-appearance: none;
				z-index: 1;
				height: 40px;
				width: 100%;
				background-color: white;
				box-shadow: none;
				border: 1px solid #E5E5E5;
				border-radius: 5px;
				display: block;
				position: relative;
				text-align: left;
				padding-left: 20px;
				overflow: hidden;
			}
		}
		.header__account {
			float: left;
			text-decoration: none;
			color: #fff;
			-moz-transition: all 0.3s ease;
			-o-transition: all 0.3s ease;
			-webkit-transition: all 0.3s ease;
			transition: all 0.3s ease;
			font-weight: 600;
			font-size: 1.285em;
			margin: 18px 0 0 0;
			&:hover {
				color: #ffd400;
				-moz-transition: all 0.3s ease;
				-o-transition: all 0.3s ease;
				-webkit-transition: all 0.3s ease;
				transition: all 0.3s ease;
			}
		}
		.header__cart {
			float: right;
			text-decoration: none;
			color: #fff;
			margin: 17px 0 0 0;
			position: relative;
			padding-left: 50px;
			padding-right: 28px;
			&:before {
				content: '';
				width: 34px;
				height: 28px;
				background-image: url(img/hbottom_icons.png);
				background-position: -20px -20px;
				display: block;
				position: absolute;
				top: -4px;
				left: -2px;
			}
			.cart__sum--numbers {
				font-weight: 600;
				font-size: 1.285em;
			}
			&:hover {
				color: #ffd400;
				&:before {
					background-position: -94px -20px;
					background-image: url(img/hbottom_icons.png);
				}
				.cart__number {
					background-color: #ffd400;
				}
			}
		}
	}
}

