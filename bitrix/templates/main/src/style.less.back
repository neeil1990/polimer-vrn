/* ============= BASKET =============*/

.basket {
	position: relative;
	padding: 30px 0 50px;	
}

.check {
	position: absolute;
	display: block;
	top: 45px;
	right: 0;
	border: none;
	font: 500 14px/16px @fira;
	text-decoration: none;
	text-align: right;
	color: @blue_l;
	&:hover, &:active {
		color: #00A1F2;
	}
}

.list {
	.l-head {
		height: 50px;
		background: #F4F6F8;
		border: 1px solid #CDD2D6;
		.lh {
			float: left;
			font: 400 14px/48px @fira;
			text-align: left;
			color: #333;
			&:nth-child(1) {
				width: 600px;
				padding-left: 120px;
			}
			&:nth-child(2) {
				width: 130px;
			}
			&:nth-child(3) {
				width: 70px;
				text-align: center;
			}
			&:nth-child(4) {
				width: 140px;
				margin-left: 45px;
				text-align: center;
			}
			&:nth-child(5) {
				width: 145px;
				margin-left: 40px;
				text-align: center;
			}
		}
	}
	.l-block {
		.l-row {
			border-bottom: 1px solid #CDD2D6;
			.l-cell {
				float: left;
				position: relative;
				padding: 30px 0;
				.txt {
					display: none;
				}
				&.img {
					display: table;
					width: 120px;
					padding: 0;
					span {
						height: 100px;
						padding: 10px;
						display: table-cell;
						vertical-align: middle;
						img {
							display: block;
							margin: 0 auto;
							max-height: 80px;
						}
					}
				}
				&.name {
					width: 480px;
					padding-right: 30px;
					font: 400 14px/20px @fira;
					color: #666;
					a {
						display: block;
						border: none;
						font: 400 14px/20px @fira;
						text-decoration: none;
						text-align: left;
						color: @blue_l;
						&:hover, &:active {
							color: #00A1F2;
						}
					}
				}
				&.price {
					width: 130px;
					font: 400 14px/20px @fira;
					color: #333;
					span {
						font-weight: 700;
					}
				}
				&.sale {
					width: 70px;
					font: 400 14px/20px @fira;
					text-align: center;
					color: #333;
					span {
						font-weight: 700;
					}
				}
				&.quan {
					width: 140px;
					margin-left: 45px;
					text-align: center;
					.quantity {
						position: relative;
						height: 30px;
						a {
							position: absolute;
							display: block;
							width: 30px;
							height: 100%;
							top: 0;
							background: transparent;
							border: 2px solid #0060bf;
							-webkit-border-radius: 15px;
							border-radius: 15px;
							font-size: 22px;
							color: #0060bf;

							&:hover{ border-color: #00A1F2; color: #00A1F2; }


							&.na{ border-color: #adadad; color: #adadad; 
								&:hover{ border-color: #adadad; color: #adadad; }
							}

							&:before{
								content: '';
								position: absolute;
								display: block;
								width: 26px;
								height: 26px;
								left: 0px;
								top: 1px;
								text-align: center;
								line-height: 26px;
							}
							&.minus{

								&:before {	
									content: '-';
								}
							}
							&.plus{
								right: 0;
								&:before {
									content: '+';
								}
							}
						}
						input[type=text] {
							position: absolute;
							margin: 0;
							padding: 0;			
							width: 40px;
							height: 100%;
							left: 50%;
							padding: 5px;
							margin-left: -20px;
							outline: none;
							border: 1px solid #CDD2D6;
							-webkit-border-radius: 5px;
							border-radius: 5px;
							font: 400 14px/20px @fira;
							text-align: center;
							color: #000;

							&:hover, &:focus{ border-color: #0060bf; }
						}
					}
				}
				&.cost {
					width: 145px;
					margin-left: 40px;
					font: 400 14px/20px @fira;
					text-align: center;
					color: #333;
					span {
						font-weight: 700;
					}
				}
				&.del {
					width: 30px;
					padding-top: 33px;
					a {
						display: block;
						width: 12px;
						height: 12px;
						background: url('img/basket/del.png') no-repeat;
						-webkit-background-size: 100%;
						-o-background-size: 100%;
						background-size: 100%;
					}
				}
			}
		}
	}
}

.options {
	position: relative;
	margin-top: 55px;
	height: 180px;
	.clear_basket {
		position: absolute;
		display: block;
		top: 12px;
		left: 0;
		padding-left: 20px;
		font: 400 14px/16px @fira;
		text-decoration: none;
		text-align: left;
		color: #666;
		&:before {
			position: absolute;
			display: block;
			content: '';
			width: 12px;
			height: 12px;
			left: 0;
			top: 50%;
			margin-top: -6px;
			background: url('img/basket/del.png') no-repeat;
			-webkit-background-size: 100%;
			-o-background-size: 100%;
			background-size: 100%;
		}
	}
	.continue_shopping {
		position: absolute;
		bottom: 16px;
		left: 0;
		font: 500 14px/16px @fira;
		text-decoration: none;
		text-align: left;
		color: #0060BF;
		&:hover, &:active {
			color: #00A1F2;
		}
	}
	.promo {
		position: absolute;
		top: 0;
		right: 340px;
		span {
			float: right;
			margin-right: 20px;
			padding: 10px 0;
			font: 400 14px/20px @fira;
			text-align: right;
			color: #333;
		}
		input[type=text] {
			float: right;
			margin-right: 10px;
			width: 120px;
			height: 40px;
			padding: 10px;
			font: 400 14px/20px @fira;
			text-align: right;
			color: #333;
			border: 1px solid #CDD2D6;
			-webkit-border-radius: 5px;
			border-radius: 5px;
			outline: none;
		}
		.apply {
			float: right;
			display: block;
			width: 120px;
			height: 40px;
			background: #0060BF;
			-webkit-border-radius: 5px;
			border-radius: 5px;
			font: 500 14px/40px @fira;
			text-decoration: none;
			text-align: center;
			color: #FFF;
			&:hover, &:active {
				background: #00A1F2;
			}
